{
  "name": "sale-events-tool",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "pnpm run generate:css --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "db:format": "prisma format",
    "db:dev:setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "db:dev:reset": "prisma db push && prisma db seed",
    "db:dev:reset:force": "prisma db push --force-reset && prisma db seed",
    "db:studio": "prisma studio",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development pnpm run build:server --watch",
    "dev:css": "cross-env NODE_ENV=development pnpm run generate:css --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "generate:css": "tailwindcss -i ./app/styles/global.css -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "prisma generate",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"pnpm exec cypress open\"",
    "test:e2e:run": "pnpm run build && cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"pnpm exec cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "dependencies": {
    "@conform-to/dom": "^0.4.0",
    "@conform-to/react": "^0.4.0",
    "@conform-to/zod": "^0.4.0",
    "@prisma/client": "^4.3.1",
    "@remix-run/express": "^1.7.4",
    "@remix-run/node": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@remix-run/server-runtime": "^1.7.4",
    "@remix-validated-form/with-zod": "^2.0.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "express": "^4.18.1",
    "isbot": "^3.5.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-typedjson": "^0.1.3",
    "remix-utils": "^3.4.0",
    "remix-validated-form": "^4.6.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.186",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.11",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cypress": "^10.8.0",
    "dotenv": "^16.0.2",
    "esbuild": "^0.15.8",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "happy-dom": "^6.0.4",
    "msw": "^0.47.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "type-fest": "^3.2.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
